<bindings xmlns:xs="http://www.w3.org/2001/XMLSchema" 
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
          xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd" 
          version="2.1" 
          xmlns="http://java.sun.com/xml/ns/jaxb">
  <globalBindings typesafeEnumMaxMembers="2000" typesafeEnumMemberName="generateError">
    <javaType name="java.time.LocalDate" xmlType="xs:date"
              parseMethod="com.helger.commons.datetime.PDTWebDateHelper.getLocalDateFromXSD"
              printMethod="com.helger.commons.datetime.PDTWebDateHelper.getAsStringXSD" />
  </globalBindings>
  <bindings schemaLocation="../resources/schemas/peppol-directory-business-card-20161123.xsd" node="/xs:schema">
    <schemaBindings>
      <package name="com.helger.pd.businesscard.v2" />
      <nameXmlTransform>
        <typeName prefix="PD2" />
      </nameXmlTransform>
    </schemaBindings>
  </bindings>
  <bindings schemaLocation="../resources/schemas/peppol-directory-business-card-20160112.xsd" node="/xs:schema">
    <schemaBindings>
      <package name="com.helger.pd.businesscard.v1" />
      <nameXmlTransform>
        <typeName prefix="PD1" />
      </nameXmlTransform>
    </schemaBindings>
  </bindings>
</bindings>
